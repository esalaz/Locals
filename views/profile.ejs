<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <!-- set viewport to device width to make site responsive -->
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- materialize css -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">
  <!-- custom styles -->
  <link href="https://fonts.googleapis.com/css?family=Kaushan+Script" rel="stylesheet">

  <link rel="stylesheet" type="text/css" href="/styles/style.css">
  <!-- jquery -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.2/jquery.min.js"></script>
  <!-- materialize js -->
   <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
   <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
 <link rel="stylesheet" href="/resources/demos/style.css">
 <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
 <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
 <script>
 $( function() {
   $( "#datepicker" ).datepicker();
 } );
 </script>
  <title>Locals</title>
</head>
<body>
  <nav class="black accent-4">
    <div class="nav-wrapper">
      <a href="#!" class="brand-logo">Locals</a>
      <a href="#" data-activities="mobile-demo" class="button-collapse"><i class="materiale-icons">Menu</i></a>
      <ul class="right hide-on-med-and-down">
        <li><a href='/feed' class="waves-effect waves-light btn">Home</a></li>
        <li><a href='/user/<%= id.toString() %>' class="waves-effect waves-light btn">Profile</a></li>
        <li><a href='/logout' class="waves-effect waves-light btn">Logout</a></li>
      </ul>
    </div>
  </nav>
  <div class="container">
    <div class="row">

      <div class="col s10 offset-s1">
          <img src="/images/blank.jpg" alt="Profile Picture" class="prof">
          <div class="form-group">
                Name: <span class="userInfo"><%= user.name %></span>
          </div>
          <div class="form-group">
                Email: <span class="userInfo"><%= user.username %></span>
          </div>
          <div class="form-group">
              Local or Traveler: <span class="userInfo"><%= user.isLocal %></span>
          </div>
          <div class="form-group">
            Age: <span class="userInfo"><%= user.age %></span>
          </div>
          <div class="form-group">
              City: <span class="userInfo"><%= user.city %></span>
          </div>
          <div class="form-group">
            Bio: <span class="userInfo"><%= user.bio %></span>
          </div>
          <!-- <br> -->
          <!-- ID of the logged in user -->
          <!-- Logged in: <%= user._id %> -->
          <!-- <br> -->
          <!-- ID of the parameter (any users profile) -->
          <!-- Profile of: <%= id %> -->
            <% if (user._id.toString() == id.toString()) { %>
        <div class="flexbox">
          <form action="/user/<%= user._id %>/update" method="GET">
            <div class="center-align">
              <input type="submit" class="waves-effect waves-light btn padding" value="Edit">
            </div>
          </form>

          <form action="/user/<%= user._id %>?_method=DELETE" method="POST">
            <div class="center-align">
              <input type="submit" class="waves-effect waves-light btn padding" value="Delete Profile">
            </div>
            </form>
            <div class="center-align">
              <a href='/feed' class="waves-effect waves-light btn">Back</a>
            </div>
            </div>
            <br>
            <% } else { %>
              <div class="flexbox">
            <div class="center-align">
              <form class="" action="/message" method="get">
              <input action='' type="submit" class="waves-effect waves-light btn padding" value="Message Me">
              </form>
            </div>
            <div class="center-align">
              <input type="submit" value="Schedule" id="datepicker" class="waves-effect waves-light btn padding">
            </div>
            <div>
              <form target="_blank" action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_top">
              <input type="hidden" name="cmd" value="_s-xclick">
              <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----">
              <input type="image" src="https://www.paypalobjects.com/en_US/i/btn/btn_paynow_SM.gif" border="0" name="submit" alt="PayPal - The safer, easier way to pay online!" class="waves-effect waves-light btn padding another">
              <img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1" height="1">
              </form>
            </div>
            <!-- <div class="center-align">
              <a href='/feed' class="waves-effect waves-light btn">Back</a>
            </div> -->
              </div>
              <br>
              <% } %>
      </div>
    </div>
  </div>



</body>
</html>
